server {
    listen 80;
    
    server_name robtest.net;

    location /css {
        proxy_http_version 1.1;
        proxy_pass http://127.0.0.1:3000/css;
        proxy_read_timeout 600s;
    }

    location / {
        proxy_http_version 1.1;
        proxy_pass http://127.0.0.1:3000;
        proxy_read_timeout 600s;
    }
}
